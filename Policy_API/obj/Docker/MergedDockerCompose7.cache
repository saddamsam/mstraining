name: dockercompose17983540833575647229
services:
  policy_api:
    build:
      context: E:\Projects\Training\API\Policy_API
      dockerfile: Policy_API/Dockerfile
    depends_on:
      sqlserverinstance:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8082"
    image: policyapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8082
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\saddam\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\saddam\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  sqlserverinstance:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Saddam@123
    expose:
      - "1433"
    hostname: sqlserverinstance
    image: mcr.microsoft.com/mssql/server
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1403"
        protocol: tcp
    volumes:
      - type: bind
        source: E:\Projects\Training\API\Policy_API\policyapi
        target: /var/opt/mssql/data
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose17983540833575647229_default